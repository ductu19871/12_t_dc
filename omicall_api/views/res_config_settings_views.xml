<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="res_config_settings_view_form" model="ir.ui.view">
		<field name="name">res.config.settings.view.form.inherit.ir_attachment_vng</field>
		<field name="model">res.config.settings</field>
		<field name="priority" eval="11" />
		<field name="inherit_id" ref="base.res_config_settings_view_form" />
		<field name="arch" type="xml">
			<xpath expr="//div[@data-key='crm']/div[2]" position="after">
				<div class="row mt16 o_settings_container">
					<div class="col-lg-6 o_setting_box">
						<div class="o_setting_left_pane">
							<field name="is_send_omi"/>
						</div>

						<div class="o_setting_right_pane crm_reveal_settings">
							<label  for="is_send_omi"/>
							<div class="text-muted">
								hahaha.
							</div>
						</div>
					</div>
				</div>
				
			</xpath>
		</field>
	</record>
</odoo>